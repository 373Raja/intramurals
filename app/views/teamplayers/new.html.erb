<h1>New teamplayer</h1>

<% form_for(@teamplayer) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :team %><br />
    <%= f.text_field :team %>
  </p>
  <p>
    <%= f.label :player %><br />
    <%= f.text_field :player %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', teamplayers_path %>